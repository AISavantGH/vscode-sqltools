{
  "name": "sqltools-driver-pg",
  "displayName": "SQLTools PostgreSQL/Redshift Driver",
  "description": "SQLTools PostgreSQL/Redshift Driver",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "mtxr",
  "license": "MIT",
  "preview": true,
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mtxr/vscode-sqltools.git",
    "directory": "packages/drivers.pg"
  },
  "bugs": {
    "url": "https://github.com/mtxr/vscode-sqltools/labels/pg"
  },
  "keywords": [
    "sqltools-driver",
    "postgre",
    "postgresql",
    "redshift",
    "aws redshift"
  ],
  "galleryBanner": {
    "theme": "dark",
    "color": "#333333"
  },
  "icon": "icon.png",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@sqltools/base-driver": "latest",
    "@types/lodash": "^4.14.123",
    "@types/pg": "^7.4.14",
    "@types/vscode": "^1.42.0",
    "@types/webpack-env": "^1.15.2",
    "lodash": "^4.17.13",
    "pg": "^7.12.1",
    "typescript": "^3.7.3"
  },
  "build": [
    {
      "entries": {
        "plugin": {
          "file": "src/ls/plugin.ts",
          "outDir": "out/ls"
        }
      },
      "type": "node"
    },
    {
      "entries": {
        "extension": {
          "file": "src/extension.ts",
          "outDir": "out"
        }
      },
      "type": "extension"
    }
  ]
}
